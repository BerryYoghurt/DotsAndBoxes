#include<stdio.h>
#include<stdlib.h>

struct player
    {
        char name[22];
        int score;
    };
 struct player player1={"rana",57};
 /////////////////////////////////////////
void file(struct player player1)
{
    FILE *F1;
    F1=fopen("D:\\CSED 23\\Programming\\Project Game\\Data5.binary","wb+"); //paths //read and writing
    if(F1==NULL)
    {
        printf("ERROR in opening the file \n");
        exit(1);
    }
    //why stop when fscanf   ???
    fwrite(&player1,sizeof(struct player), 1,F1);  //numbersData???
    if(fprintf != 0)
        printf("contents to file written successfully !\n");
    else
        printf("error writing file !\n");
    fclose(F1);
}
int main(){
file(player1);  //player_num???
return 0;
}
